{
    "type":"sqlite",
    "database":"./src/database/database.sqlite",

    "entities": [
        "src/models/**/*.ts"
     ],
     "migrations": [
        "src/database/migration/**/*.ts"
     ],
     "subscribers": [
        "src/subscriber/**/*.ts"
     ],
     "cli": {
        "entitiesDir": "src/models",
        "migrationsDir": "src/database/migration",
        "subscribersDir": "src/subscriber"
     }

}